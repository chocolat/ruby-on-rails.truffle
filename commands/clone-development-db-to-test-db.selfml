(trigger
    (key [[ctrl][shift]|]))

(name [Clone Development DB to Test DB])

(only-in [source.ruby.rails, source.yaml])

(input nothing)

(output html)

(save nothing)

(script [RUBYLIB="$TM_BUNDLE_SUPPORT/lib:$RUBYLIB"
"${TM_RUBY:=ruby}" -- "${TM_BUNDLE_SUPPORT}/bin/rake_helper.rb" db:test:clone
])