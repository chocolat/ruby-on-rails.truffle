(trigger
    (key [[ctrl][shift]|]))

(name [Load Fixtures (Test DB)])

(only-in [source.ruby.rails, source.yaml])

(input nothing)

(output html)

(save nothing)

(script [RUBYLIB="$TM_BUNDLE_SUPPORT/lib:$RUBYLIB"
"${TM_RUBY:=ruby}" -- "${TM_BUNDLE_SUPPORT}/bin/rake_helper.rb" db:fixtures:load -v RAILS_ENV -a test
])